<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--
         The set of colors that the user can select to be shown when their LevelUp code is scanned.
         Note that the order of these elements is very important and cannot be changed, because the
         color is selected by array indexing.
    -->
    <array name="levelup_dock_colors">
        <item>@color/dock_soothing_green</item>
        <item>@color/dock_atomic_lime</item>
        <item>@color/dock_tango_red</item>
        <item>@color/dock_galactic_blue</item>
        <item>@color/dock_purple_haze</item>
        <item>@color/dock_yellow_submarine</item>
        <item>@color/dock_arctic_teal</item>
        <item>@color/dock_sick_flamingo</item>
        <item>@color/dock_mint_julep</item>
        <item>@color/dock_ninja_orange</item>
    </array>

    <!-- Set of tip percentages ranging from 0% to 50%. -->
    <integer-array name="levelup_tip_percentages">
        <item>0</item>
        <item>5</item>
        <item>10</item>
        <item>15</item>
        <item>18</item>
        <item>20</item>
        <item>25</item>
        <item>30</item>
        <item>40</item>
        <item>50</item>
    </integer-array>

</resources>